      <%@teste = '' %> 
      <% @cadastros.each do |cadastro| %>
         <% if @teste != cadastro.cadastro_id %>
            <div class="tree">
               <ul>
                  <li>
                     <div class="family">
                        <div class="person child male">
                           <div class="name"><%= cadastro.patrocinador  %></div>
                        </div>
                        <ul>
                         <li>
                            <div class="family" style="width: 172px">
                               <div class="person child male">
                                  <div class="name"><%= cadastro.nome_usuario  %></div>
                               </div>
                            </div>
                         </li>
               <%@teste = cadastro.cadastro_id%>
         <%end%>
      <%end%>
      </li>
    </ul>
</div>

<%= link_to 'New Cadastro', new_cadastro_path %>

